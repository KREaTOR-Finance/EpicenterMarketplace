    "react-virtualized": "^9.22.5",
    "date-fns": "^2.30.0",
    "class-variance-authority": "^0.7.0",
    "lucide-react": "^0.292.0",
    "react-helmet-async": "^1.3.0",
    "react-error-boundary": "^4.0.11",
    "axios": "^1.6.0",
    "ipfs-http-client": "^60.0.1",
    "arweave": "^1.14.4"
