{
  "name": "seismic-epicenter-backend",
  "version": "1.0.0",
  "description": "Backend API and indexer for Seismic Epicenter NFT Marketplace",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:seed": "ts-node src/scripts/seed.ts",
    "indexer:start": "ts-node src/indexer/indexer.ts",
    "indexer:dev": "nodemon src/indexer/indexer.ts"
  },
  "keywords": [
    "nft",
    "marketplace",
    "sei",
    "solana",
    "graphql",
    "api",
    "indexer"
  ],
  "author": "Seismic Epicenter Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "apollo-server-express": "^3.12.1",
    "graphql": "^16.8.1",
    "type-graphql": "^2.0.0-beta.3",
    "reflect-metadata": "^0.1.13",
    "prisma": "^5.3.1",
    "@prisma/client": "^5.3.1",
    "ethers": "^6.8.1",
    "@solana/web3.js": "^1.87.6",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "redis": "^4.6.10",
    "bull": "^4.12.0",
    "ipfs-http-client": "^60.0.1",
    "arweave": "^1.14.4",
    "ws": "^8.14.2",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.8.10",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/ws": "^8.5.8",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
} 